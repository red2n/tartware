# Tartware Load Test Environment Configuration
# Copy to .env and fill in values

# API Gateway URL
GATEWAY_URL=http://localhost:8080

# Authentication token (get from /v1/auth/login)
API_TOKEN=

# Test Tenant Configuration
TENANT_ID=11111111-1111-1111-1111-111111111111
PROPERTY_ID=22222222-2222-2222-2222-222222222222
ROOM_TYPE_ID=44444444-4444-4444-4444-444444444444

# Test Configuration
VUS=50
DURATION=5m
RAMP_UP=30s

# InfluxDB Configuration (for metrics storage)
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=tartware-token
INFLUXDB_ORG=tartware
INFLUXDB_BUCKET=k6
