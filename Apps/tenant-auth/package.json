{
  "name": "@tartware/tenant-auth",
  "version": "0.1.0",
  "description": "Shared Fastify tenant auth context plugin",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jwt": {
      "types": "./dist/jwt.d.ts",
      "import": "./dist/jwt.js"
    },
    "./membership": {
      "types": "./dist/membership.d.ts",
      "import": "./dist/membership.js"
    },
    "./auth-plugin": {
      "types": "./dist/auth-plugin.d.ts",
      "import": "./dist/auth-plugin.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "biome": "biome check src",
    "biome:fix": "biome format src --write",
    "knip": "knip --tsConfig tsconfig.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@tartware/schemas": "workspace:*",
    "fastify-plugin": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10"
  },
  "peerDependencies": {
    "fastify": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
