{
  "name": "@tartware/candidate-pipeline",
  "version": "0.1.0",
  "description": "Reusable candidate pipeline framework for recommendation systems (inspired by X's architecture)",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "typecheck": "tsc --noEmit",
    "biome": "biome check src",
    "biome:fix": "biome format src --write",
    "knip": "knip --tsConfig tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist coverage *.tsbuildinfo"
  },
  "dependencies": {
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^20.19.31",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
