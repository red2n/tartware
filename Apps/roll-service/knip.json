{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/index.ts"],
  "project": ["src/**/*.ts"],
  "ignore": [
    "src/tests/**/*",
    "src/repositories/__tests__/**/*"
  ],
  "ignoreDependencies": [
    "@biomejs/biome",
    "@tartware/openapi",
    "@tartware/schemas",
    "@tartware/telemetry",
    "@tartware/config",
    "p-retry",
    "zod",
    "tsx"
  ],
  "ignoreBinaries": ["biome", "eslint", "knip", "tsc", "tsx"],
  "ignoreExportsUsedInFile": true
}
