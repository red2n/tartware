{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/main.ts"],
  "project": ["src/**/*.ts", "src/**/*.html"],
  "rules": {
    "files": "off",
    "dependencies": "off",
    "devDependencies": "off",
    "exports": "off",
    "types": "off",
    "unlisted": "off",
    "binaries": "off",
    "classMembers": "off",
    "enumMembers": "off",
    "nsExports": "off",
    "nsTypes": "off",
    "duplicates": "off",
    "optionalPeerDependencies": "off",
    "unresolved": "off"
  }
}
